<!DOCTYPE html>
<html lang="en">

  <%- include('partials/header'); %>
  <body>
    <%- include('partials/header'); %>
  </body>
      <!-- Page-specific (main) content here -->
  <main>


    <div class="container m-3">
      <div class="map_row row">
        <% for (let i=0; i<maps.length; i++) { %>

         <div class="add_map" class="col">
          <a href="/<%= maps[i].id %>">
            <div class="card" >
              <img src="<%= maps[i].image_url %>" alt="...">
              <div class="card-body">
                <p class="card-text"><%=maps[i].title%></p>
              </div>
            </div>
            </a>
          </div>
         <% } %>
      </div>
    </main>
  </body>


  <footer><%- include('partials/footer'); %></footer>

</html>
